{
  "name": "rounding-ladder",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "module": "dist/src/index.esm.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "format": "prettier --write src/ tests/",
    "build": "tsc",
    "clean": "rm -rf dist/",
    "test": "jest"
  },
  "author": "Adam Lett",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.9",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "./*"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "clearMocks": true
  }
}
